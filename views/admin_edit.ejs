<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Show view</title>
</head>
<body>
	<h2><%= type %></h2>
	<% if (items && items.length) { %>
      <ul>
          <% items.forEach(function(item){ %>
            <li><label><%= item.key %>: </label><input type="text" value="<%= item.value %>"/></li>
          <% }) %>
      </ul>
  <% } %>

  <ul>
    <li><a href="#">Save</a></li>
    <li><a href="./">Cancel</a></li>
  </ul>
  <script src="/javascripts/events.js"></script>
</body>
</html