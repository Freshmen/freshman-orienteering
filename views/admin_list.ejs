<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>List view</title>
</head>
<body>
	<h2><%= type %></h2>
	<% if (items.length) { %>
        <ul>
          <% items.forEach(function(item){ %>
            <li><a href="<%= item._id %>/"><%= item.title %></a></li>
          <% }) %>
        </ul>
    <% } %>
    <ul>
      <li><a href="./create">Add</a></li>
  </ul>
  <script src="/javascripts/events.js"></script>
</body>
</html