<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title></title>
<link rel='stylesheet' href='/stylesheets/mobile.css'/>
</head>
<body>
	<h1><%= title %></h1>
    <div>
    <div id="profileWrap">
            <div id="profile"></div>
        </div>

        <div id="houseWrap">
            <div id="house"></div>
        </div>
    </div>
	<p>Currently logged in user is: 
		<span>
			<% if(user && user.name) { %>
			 user has a name, the name is: 
				<%= user.name %>
			<% } else { %>
				no user found.
			<% } %>
		</span>
	</p>
	<h2>Enrolled events</h2>
	<ul>
		<% enrolled.forEach(function(evt){ %>
			<li><a href="/events/<%= evt._id %>"><%= evt.title %></a></li>
		<%})%> 
	</ul>
	<h2>All events</h2>
	<ul>
		<% unenrolled.forEach(function(evt){ %>
			<li><a href="/events/<%= evt._id %>"><%= evt.title %></a></li>
		<%})%> 
	</ul>
</body>
</html>