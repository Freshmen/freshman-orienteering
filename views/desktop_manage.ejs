<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title><%= title %></title>
	<link rel='stylesheet' href='/stylesheets/desktop.css' />
	<link rel='stylesheet' href='/stylesheets/notifier-base.css' />
	<link rel='stylesheet' href='/stylesheets/notifier-theme-plastic.css' />
	<link rel='stylesheet' href='/dependencies/jquery-tablesorter/themes/blue/style.css'> 
	<link  rel='stylesheet' href='http://fonts.googleapis.com/css?family=Arvo'>
	<link rel="stylesheet" href='//cdnjs.cloudflare.com/ajax/libs/font-awesome/3.0.2/css/font-awesome.min.css'>
    <script src="/javascripts/management_helper.js"></script>
    <script data-main="/javascripts/can.helper.js" src="/dependencies/libs/require-jquery.js"></script>
   	<script src="/javascripts/moment.min.js"></script>
</head>
<body>
	<header id="banner">
		<div id="header-logo">
				<a href="/desktop" id="logo-link">
					<img src="/images/gamified_logo_small.png" alt="Logo" id="logo">
				</a>
			</div>
		<div id="organizerName">
			<% if(user && user.name) { %>
				<span><%= user.name %> logged in </span><a href="/logout">(log out)</a>
			<% } else { %>
				<span>You are not logged in.</span>
			<% } %>
		</div>
		<nav>
			<li><a href="/desktop">Home</a></li>
			<li><a href="/desktop/create">Create Event</a></li>
			<li class="active"><a href="/desktop/manage">Manage Events</a></li>
		</nav>
	</header>

	<div id="ajaxLoader"><img src="/images/ajax-loader.gif"></img></div>
	<div id="content"></div>

	<footer></footer>

	<script src="/dependencies/underscore-1.3.3.min.js"></script>
	<script src="/dependencies/backbone-0.9.2.js"></script>
	<script src="/dependencies/backbone.notifier.js"></script>
	<script src="/dependencies/jquery-tablesorter/jquery.tablesorter.js"></script>

	<script>
		$(function(){
			getEventData();
		});
	</script>
	
</body>
</html>
