<section id="content" class="listbox">
       <span id="contentListClose" class="delete">X</span>
       <nav id="contentNav">
       <!-- if statement -->
       <!-- check the first object if it is the type of Checkpoint, Event or others -->
       <% if (content[0] && content[0].type == "Checkpoint") {%>  
            	<ul id="checkpoints" class="contentList" data-tag="checkpoints">
       <%} else if (content[0] && content[0].type == "Event"){ %>   
       			<ul id="events" class="contentList" data-tag="events">
       <%} else{%>
       			<ul id="currentEvent" class="contentList" data-tag="event_description">
       <%} %>

       <!-- if statement -->
       <% if (content[0]) {%>
	       <!-- for statement -->
          	<% for(var i = 0; i<content.length; i++) {%>
	            <li data-index= <%=i%> ><a href="#"><%= content[i].title%></a></li>  
	            <% if (content[i].hasOwnProperty('organizer')) {%>  
	            	<span class="listSmall" id="organizer"><%= 'Organizer: ' + content[i].organizer%></span>
            	<%}%>         
          	<% } %>
	       <!-- end of for statement -->
       
       <%} else {%>
       		<li><h2>Event Name: <%= content.title %></h2></li>
       		<li><p>Description: <%= content.description %><p></li>
          	<li data-tag=<%=content._id%> id="showCheckpoints"><a href="#">Show Checkpoints</a></li>
          	<li><div id="checkPointWrap"></div></li>
          	<li id="goBack"><a href="#">Go Back</a></li>
          	<li id="enrol"><a href="#">Enrol</a></li>
       <%}%>
        </ul>
      
      </nav>
 </section>