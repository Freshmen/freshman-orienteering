body
    background: #F5F4EF
    color: #000305
    font-size: 87.5%
    font-family: 'Arvo', serif
    line-height: 1.429
    margin: 0
    padding: 0
    text-align: left

.body
    clear: both
    margin: 0 auto
    width: 800px

.button
    background-color: #000000
    border-radius: 5px
    display: inline-block
    color: #ffffff
    padding: 6px 20px
    text-decoration: none
    outline: none
    border-width: 0px
    font-size: 16px
    font-family: 'Arvo', serif

.button:hover 
    background-color: #1b7ae0

input
    font-family: 'Arvo', serif
    autocomplete: "off"

#event_form
    float: left
    width: 450px

#mapArea
    height: 400px
    width: 500px
    float: right

#content
    width: 1000px
    margin-left: auto
    margin-right: auto
    padding: 20px

#mapArea2
    height: 390px
    width: 390px
    border: 0px solid #eaeaea

#checkpoint_holder
    width: 1000px
    padding: 20px
    clear: both
    margin-left: auto
    margin-right: auto

#banner
    margin: 0 auto
    text-align: center

#banner h1
    font-size: 3.571em
    line-height: .6
    padding: 20px

#banner h1 a:link, #banner h1 a:visited
    color: #000305
    font-weight: bold
    text-decoration: none

#banner h1 a:hover, #banner h1 a:active
    background: none
    color: #1B7AE0
    text-shadow: none

#banner nav
    background: #000305
    font-size: 1.143em
    height: 40px
    line-height: 30px
    margin: 0 auto 2em auto
    display: inline-block
    border-radius: 5px

#banner nav li
    float: left
    display: inline-block

#banner nav a:link, #banner nav a:visited
    color: #fff
    display: inline-block
    height: 30px
    padding: 5px 1.5em
    text-decoration: none

#banner nav a:hover, #banner nav a:active, #banner nav .active a:link, #banner nav .active a:visited
    background: #1B7AE0
    color: #fff
    text-shadow: none !important

#banner nav li:first-child a
    border-top-left-radius: 5px
    border-bottom-left-radius: 5px

#banner nav li:last-child a
    border-top-right-radius: 5px
    border-bottom-right-radius: 5px
h2
    font-size: 1.571em
h3 
    font-size: 1.429em
h4 
    font-size: 1.286em
h5 
    font-size: 1.143em
h6 
    font-size: 1em
h2, h3, h4, h5, h6
    font-weight: 400
    line-height: 1.1
    margin-bottom: .8em

a
    outline: 0
a img
    border: 0px
    text-decoration: none

a:link, a:visited
    color: #1B7AE0
    padding: 0 1px
    text-decoration: underline

a:hover, a:active
    background-color: #1B7AE0
    color: #fff
    text-decoration: none
    text-shadow: 1px 1px 1px #333

p
    margin-bottom: 1.143em
* p:last-child
    margin-bottom: 0

thead th
    padding: .5em .4em
    text-align: left

thead td

tbody td
    padding: .5em .4em

.drag-container 
    width:100%;
    margin:0px auto;
    background:#fff url(../images/leftside.gif) repeat-y left top;
    float:top;	

.top
    width:100%;
    height:20px;
    background:url(../images/top.gif) no-repeat left top;

.top span
    display:block;
    position:relative;
    height:20px;
    background:url(../images/top-right.gif) no-repeat right top;

.center-content
    position:relative;
    background:url(../images/rightside.gif) repeat-y right top;
    padding:1px 20px 1px 25px;

.bottom
    height:60px;
    background:url(../images/bottom.gif) no-repeat left bottom;

.bottom span
    display:block;
    position:relative;
    height:60px;
    background:url(../images/bottom-right.gif) no-repeat right top;

